{
    "source": {
      "include": ["src"],
      "exclude": ["node_modules"],
      "includePattern": ".jsx$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "./README.md"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }